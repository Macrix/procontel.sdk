{
  "name": "tp1-prototype",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start-pwa": "ng build --configuration production && http-server -p 8080 -c-1 dist/rwenuc",
    "generate-icons": "ngx-pwa-icons -i ./src/assets/icons/icon.png",
    "generate-models": "nswag openapi2tsclient /input:./swagger.json /output:./src/app/core/generated-models/index.ts /generateClientClasses:false /generateResponseClasses:false",
    "generate-locale": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json ./src/assets/i18n/de.json --clean --sort --format namespaced-json --marker _"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.2",
    "@angular/common": "~13.0.2",
    "@angular/compiler": "~13.0.2",
    "@angular/core": "~13.0.2",
    "@angular/forms": "~13.0.2",
    "@angular/platform-browser": "~13.0.2",
    "@angular/platform-browser-dynamic": "~13.0.2",
    "@angular/router": "~13.0.2",
    "@angular/service-worker": "~13.0.2",
    "@microsoft/signalr": "6.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@angular/cdk": "^13.0.2",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/material": "^13.0.2",
    "@zxing/browser": "0.0.10",
    "@zxing/library": "^0.18.6",
    "@zxing/ngx-scanner": "^3.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.2",
    "@types/jasmine": "~3.10.2",
    "@types/node": "^16.11.9",
    "jasmine-core": "~3.10.1",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "nswag": "13.14.4",
    "@misternick/ngx-translate-extract": "^7.0.4",
    "typescript": "4.4.4"
  }
}
